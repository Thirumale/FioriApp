{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "BasicFiori-Routing",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZMATERIAL_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"ach": "CA-UI5-DOC"
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "BasicFiori-Routing.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.38.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.38.0"
				},
				"sap.m": {
					"minVersion": "1.38.0"
				},
				"sap.ushell": {
					"minVersion": "1.38.0"
				},
				"sap.collaboration": {
					"minVersion": "1.38",
					"lazy": true
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"config": {
			"invoiceLocal": "http://northwind.servicestack.net/customers?format=json"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "BasicFiori-Routing.i18n.i18n"
				}
			},
			"MainService": {
				"dataSource": "mainService",
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "BasicFiori-Routing.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [{
				"pattern": "",
				"name": "material",
				"target": "material"
			}, {
				"pattern": "material/{MaterialId}",
				"name": "materialDetails",
				"target": [
					"material",
					"materialDetails"
				]
			}, 
			{
				"pattern": "materialCreate",
				"name": "materialCreate",
				"target": [
					"material",
					"materialCreate"
				]
			},{
				"pattern": "customerDetail/{customerId}",
				"name": "customerDetail",
				"target": "customerDetail"
			}, {
				"pattern": "customerOrder/{orderDetailsPath}",
				"name": "customerOrder",
				"target": "customerOrder"
			}],
			"targets": {
				"material": {
					"viewName": "Material",
					"controlAggregation": "masterPages"
				},
				"materialDetails": {
					"viewName": "MaterialDetails",
					"controlAggregation": "detailPages"
				},
				"materialCreate":{
						"viewName": "materialCreate",
					"controlAggregation": "detailPages"
				},
				"overview": {
					"viewName": "Overview",
					"controlAggregation": "masterPages"
				},
				"customerDetail": {
					"viewName": "CustomerDetail",
					"controlAggregation": "detailPages"
				},
				"customerOrder": {
					"viewName": "CustomerOrder",
					"controlAggregation": "detailPages"
				},
				"welcome": {
					"viewName": "Welcome",
					"controlAggregation": "detailPages"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}